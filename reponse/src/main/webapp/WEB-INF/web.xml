<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee
                      http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0"
         metadata-complete="true">
      
    <servlet>
      <servlet-name>filedown</servlet-name>
      <servlet-class>com.bantanger.reponse.FileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
      <servlet-name>filedown</servlet-name>
      <url-pattern>/filedown</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>img</servlet-name>
        <servlet-class>com.bantanger.reponse.ImageServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>img</servlet-name>
        <url-pattern>/img</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>red</servlet-name>
        <servlet-class>com.bantanger.reponse.RedirectServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>red</servlet-name>
        <url-pattern>/red</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>req</servlet-name>
        <servlet-class>com.bantanger.reponse.RequestTest</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>req</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
</web-app>